{
  "name": "minter-console-web",
  "version": "1.0.0",
  "description": "Minter Console Website",
  "author": "shrpne <shrpne@gmail.com>",
  "private": true,
  "scripts": {
    "prod": "npm run production",
    "production": "npm run build",
    "dev": "gulp & nuxt --spa --port=3002 --hostname=localhost",
    "build": "gulp once && nuxt build --spa",
    "start": "nuxt start --spa --port=3002 --hostname=localhost",
    "generate": "nuxt generate",
    "analyze": "gulp once && nuxt build --spa --analyze",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .js,.vue --ignore-path .gitignore --fix .",
    "test": "gulp once && NODE_ENV=test jest --runInBand",
    "test:debug": "DEBUG=true npm run test",
    "test:debug:skip-build": "DEBUG=true NUXT_SKIP_SELF_BUILD=true npm run test",
    "precommit": "echo 'Pre-commit checks...' && npm run lint"
  },
  "pre-commit": [
    "precommit"
  ],
  "dependencies": {
    "@material/list": "^1.0.1",
    "@material/menu": "^1.0.1",
    "@material/snackbar": "^1.0.1",
    "aes-js": "^3.1.2",
    "axios": "^0.18.0",
    "clipbrd": "^1.0.2",
    "cookie": "^0.3.1",
    "date-fns": "^2.0.0-alpha.27",
    "entity-decode": "^2.0.2",
    "js-cookie": "^2.2.0",
    "lodash-es": "^4.17.11",
    "minter-footer-links": "github:MinterTeam/minter-footer-links#v2.2.0",
    "minter-js-org": "^0.3.0",
    "minter-js-sdk": "^0.13.0",
    "minterjs-tx": "^4.4.0",
    "minterjs-util": "0.6.1",
    "minterjs-wallet": "^3.0.1",
    "nuxt": "^2.4.5",
    "nuxt-i18n": "^5.8.5",
    "pretty-num": "^0.2.2",
    "qrcode.vue": "^1.6.1",
    "v-autosize": "^1.0.1",
    "vue-autonumeric": "^1.2.6",
    "vue-imask": "^4.1.5",
    "vue-simple-svg": "^1.3.2",
    "vuelidate": "^0.7.4",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "autoprefixer": "^9.5.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "beeper": "^1.1.1",
    "cross-env": "^5.2.0",
    "del": "^4.0.0",
    "dotenv": "^7.0.0",
    "eslint": "^5.15.2",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-vue": "^5.2.2",
    "faker": "^4.1.0",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.0",
    "gulp-cache": "^1.1.1",
    "gulp-clean-css": "^4.0.0",
    "gulp-imagemin": "^5.0.3",
    "gulp-less": "^4.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "imagemin-mozjpeg": "^8.0.0",
    "jest": "^24.5.0",
    "jest-environment-node": "^24.5.0",
    "mkdirp": "^0.5.1",
    "postcss-normalize": "^7.0.1",
    "pre-commit": "^1.2.2",
    "puppeteer": "^1.13.0",
    "rimraf": "^2.6.3"
  },
  "browserslist": [
    "> 0.5%",
